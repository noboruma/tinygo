{
    "goos": "linux",
    "goarch": "arm",
    "build-tags": [
        "tkey",
        "tinygo.riscv32",
        "tinygo.riscv",
        "baremetal",
        "linux",
        "arm"
    ],
    "llvm-target": "riscv32-unknown-none",
    "cpu": "generic-rv32",
    "target-abi": "ilp32",
    "features": "+c,+zmmul",
    "cflags": [
        "-mcmodel=medany",
        "-march=rv32iczmmul",
        "-Werror",
        "-mno-relax",
        "-fno-exceptions",
        "-fno-unwind-tables",
        "-fno-asynchronous-unwind-tables",
        "-ffunction-sections",
        "-fdata-sections"
    ],
    "ldflags": [
        "-melf32lriscv",
        "--gc-sections"
    ],
    "linkerscript": "targets/tkey.ld",
    "gdb": [
        "gdb-multiarch"
    ],
    "scheduler": "none",
    "default-stack-size": 2048,
    "gc": "leaking",
    "linker": "ld.lld",
    "rtlib": "compiler-rt",
    "libc": "picolibc",
    "extra-files": [
        "src/runtime/asm_riscv.S",
        "src/device/tkey/crt0.S"
    ],
    "flash-command": "tkey-runapp {bin}",
    "serial": "uart"
}
